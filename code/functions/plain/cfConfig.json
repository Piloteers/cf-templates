{
  "{{functionName}}Function": { 
    "Type": "AWS::Serverless::Function",
    "Properties": {
        "CodeUri": "../../dist/{{functionName}}/{{functionName}}.zip",
        "FunctionName": {
					"Fn::Join": [
						"-",
						[
							{
								"Ref": "AWS::StackName"
							},
							"{{functionName}}"
						]
					]
				},
        "Handler": "index.default",
        "Runtime": "nodejs8.10",
        "MemorySize": 128,
        "Timeout": 30,
        "AutoPublishAlias": "current",
        "Events": {}
    }
  }
}